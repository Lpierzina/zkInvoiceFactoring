{"noir_version":"1.0.0-beta.6+9580a12c1184435af460a768d40a09f0f4b9df63","hash":"6832936508804908968","abi":{"parameters":[{"name":"total_invoices","type":{"kind":"integer","sign":"unsigned","width":32},"visibility":"private"},{"name":"paid_invoices","type":{"kind":"integer","sign":"unsigned","width":32},"visibility":"private"},{"name":"threshold_percent","type":{"kind":"integer","sign":"unsigned","width":32},"visibility":"private"},{"name":"total_debt","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"total_income","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"dti_threshold_bp","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"dso","type":{"kind":"integer","sign":"unsigned","width":32},"visibility":"private"},{"name":"dso_threshold","type":{"kind":"integer","sign":"unsigned","width":32},"visibility":"private"},{"name":"ar_over60","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"ar_total","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"ar_pct_threshold_bp","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"revenue12mo","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"revenue_threshold","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"largest_cust_sales","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"total_sales","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"},{"name":"concentration_threshold_bp","type":{"kind":"integer","sign":"unsigned","width":64},"visibility":"private"}],"return_type":{"abi_type":{"kind":"array","length":6,"type":{"kind":"boolean"}},"visibility":"public"},"error_types":{"7233212735005103307":{"error_kind":"string","string":"attempt to multiply with overflow"}}},"bytecode":"","debug_symbols":"","file_map":{"50":{"source":"fn main(\n    total_invoices: u32,\n    paid_invoices: u32,\n    threshold_percent: u32,\n\n    total_debt: u64,\n    total_income: u64,\n    dti_threshold_bp: u64,\n\n    dso: u32,\n    dso_threshold: u32,\n\n    ar_over60: u64,\n    ar_total: u64,\n    ar_pct_threshold_bp: u64,\n\n    revenue12mo: u64,\n    revenue_threshold: u64,\n\n    largest_cust_sales: u64,\n    total_sales: u64,\n    concentration_threshold_bp: u64\n) -> pub [bool; 6] {\n    // 1. Invoice Reliability\n    assert(total_invoices > 0);\n    let paid_percent: u32 = (paid_invoices * 100) / total_invoices;\n    let reliable: bool = paid_percent >= threshold_percent;\n\n    // 2. DTI\n    let dti_pass: bool = if (total_income > 0) {\n        ((total_debt * 10_000) / total_income) <= dti_threshold_bp\n    } else {\n        false\n    };\n\n    // 3. DSO (patched to always pass if dso_threshold == 0)\n    let dso_pass: bool = if (dso_threshold == 0) {\n        true\n    } else {\n        dso < dso_threshold\n    };\n\n    // 4. AR > 60 Days (patched to always pass if ar_pct_threshold_bp == 0)\n    let pct_over60: u64 = if (ar_total > 0) {\n        (ar_over60 * 10_000) / ar_total\n    } else {\n        0\n    };\n    let ar_aging_pass: bool = if (ar_pct_threshold_bp == 0) {\n        true\n    } else {\n        pct_over60 < ar_pct_threshold_bp\n    };\n\n    // 5. Revenue (patched to always pass if revenue_threshold == 0)\n    let revenue_pass: bool = if (revenue_threshold == 0) {\n        true\n    } else {\n        revenue12mo > revenue_threshold\n    };\n\n    // 6. Concentration (patched to always pass if concentration_threshold_bp == 0)\n    let concentration_pct: u64 = if (total_sales > 0) {\n        (largest_cust_sales * 10_000) / total_sales\n    } else {\n        0\n    };\n    let concentration_pass: bool = if (concentration_threshold_bp == 0) {\n        true\n    } else {\n        concentration_pct < concentration_threshold_bp\n    };\n    \n\n    [reliable, dti_pass, dso_pass, ar_aging_pass, revenue_pass, concentration_pass]\n}\n","path":"/mnt/c/Users/aeria/Videos/zkInvoiceFactoring/invoice_reliability/src/main.nr"}},"names":["main"],"brillig_names":["directive_integer_quotient","directive_invert"]}